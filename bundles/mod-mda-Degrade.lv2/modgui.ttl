@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mda/Degrade>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-degrade.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-degrade.css> ;
        modgui:screenshot <modgui/screenshot-mda-degrade.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-degrade.png> ;
        modgui:brand "MDA" ;
        modgui:label "Degrade" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "lava" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "headroom" ;
            lv2:name "Headrm" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "rate" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "output" ;
            lv2:name "Output" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "quant" ;
            lv2:name "Quant" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "post_filt" ;
            lv2:name "Post Filt" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "non_lin" ;
            lv2:name "Non-Lin" ;
        ] ;
    ] .