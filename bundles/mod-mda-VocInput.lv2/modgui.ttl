@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix ui:     <http://lv2plug.in/ns/extensions/ui#> .

<http://moddevices.com/plugins/mda/VocInput>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-vocinput.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-vocinput.css> ;
        modgui:screenshot <modgui/screenshot-mda-vocinput.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-vocinput.png> ;
        modgui:brand "MDA" ;
        modgui:label "VocInput" ;
        modgui:model "boxy" ;
        modgui:panel "5-knobs" ;
        modgui:color "darkblue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "tracking" ;
            lv2:name "Tracking" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "pitch" ;
            lv2:name "Pitch" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "breath" ;
            lv2:name "Breath" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "s_thresh" ;
            lv2:name "S Thresh" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "max_freq" ;
            lv2:name "Max Freq" ;
        ] ;
    ] .