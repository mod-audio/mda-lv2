@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mda:  <http://moddevices.com/plugins/mda/> .
@prefix mdap: <http://moddevices.com/plugins/mda/presets#> .
@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pg:   <http://lv2plug.in/ns/ext/port-groups#> .

mda:
    a doap:Project ;
    lv2:symbol "mda" ;
    doap:name "MDA LV2" ;
    doap:shortdesc "An LV2 port of the MDA plugins." ;
    doap:homepage <http://drobilla.net/software/mda-lv2> ;
    doap:maintainer [
        foaf:name "MOD Team";
        foaf:homepage <http://moddevices.com>;
        foaf:mbox <mailto:devel@moddevices.com>;
    ] ;
    doap:developer [
        a foaf:Person ;
        foaf:name "Paul Kellett" ;
        foaf:mbox <mailto:paul@mda-vst.com>
    ] .

mda:mainIn
    a pg:StereoGroup ,
        pg:InputGroup ;
    lv2:name "Input" ;
    lv2:symbol "in" .

mda:mainOut
    a pg:StereoGroup ,
        pg:OutputGroup ;
    pg:source mda:mainIn ;
    lv2:name "Output" ;
    lv2:symbol "out" .

mda:Vocoder
    a lv2:Plugin ;
    rdfs:seeAlso <Vocoder.ttl> ,
        <modgui.ttl> ;
    lv2:binary <Vocoder@LIB_EXT@> .

mdap:Vocoder-vocoder
    a pset:Preset ;
    lv2:appliesTo mda:Vocoder ;
    rdfs:seeAlso <presets.ttl> .

mdap:Vocoder-16-band-vocoder
    a pset:Preset ;
    lv2:appliesTo mda:Vocoder ;
    rdfs:seeAlso <presets.ttl> .

mdap:Vocoder-old-vocoder
    a pset:Preset ;
    lv2:appliesTo mda:Vocoder ;
    rdfs:seeAlso <presets.ttl> .

mdap:Vocoder-choral-vocoder
    a pset:Preset ;
    lv2:appliesTo mda:Vocoder ;
    rdfs:seeAlso <presets.ttl> .

mdap:Vocoder-pad-vocoder
    a pset:Preset ;
    lv2:appliesTo mda:Vocoder ;
    rdfs:seeAlso <presets.ttl> .
