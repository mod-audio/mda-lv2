@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mda: <http://moddevices.com/plugins/mda/> .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix time: <http://lv2plug.in/ns/ext/time#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

mda:mainIn
    a pg:StereoGroup ,
        pg:InputGroup ;
    lv2:name "Input" ;
    lv2:symbol "in" .

mda:mainOut
    a pg:StereoGroup ,
        pg:OutputGroup ;
    pg:source mda:mainIn ;
    lv2:name "Output" ;
    lv2:symbol "out" .

mda:DubDelay
    a lv2:Plugin ,
        lv2:DelayPlugin ;
    lv2:project mda: ;
    lv2:symbol "DubDelay" ;
    doap:name "MDA DubDelay" ;
    doap:shortdesc "Delay with feedback saturation and time/pitch modulation" ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput mda:mainIn ;
    pg:mainOutput mda:mainOut ;
    lv2:minorVersion 0;
    lv2:microVersion 7;
    mod:brand "MDA" ;
    mod:label "DubDelay" ;
    rdfs:comment """
The DubDelay is the go-to pedal for musicians who look for a delay pedal that adds something special to their sound.
The unique features of the DubDelay are its LFO and Pitch effect, and the modulation it creates. Perfect for clean sounds where you would want to add a vibrato-like effect to the mix. Or change the Pitch to create sounds you have never heard before.

Features:
Modeled by MDA
""";


    lv2:port[
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:name "Delay" ;
        lv2:shortName "Time" ;
        lv2:symbol "delay" ;
        lv2:default 460 ;
        lv2:minimum 0.0 ;
        lv2:maximum 7500.0 ;
        lv2:portProperty epp:logarithmic ;
        units:unit units:ms ;
        lv2:portProperty  mod:tapTempo , mod:tempoRelatedDynamicScalePoints;
        rdfs:comment "Delay time (delay is mono for authenticity!)"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:name "Feedback" ;
        lv2:symbol "feedback" ;
        lv2:default 55.0 ;
        lv2:minimum -110.0 ;
        lv2:maximum 110.0 ;
        units:unit units:pc ;
        rdfs:comment "Feedback - with soft saturation at maximum left position, hard clipping at maximum right"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "Fb Tone" ;
        lv2:shortName "Tone" ;
        lv2:symbol "fb_tone" ;
        lv2:default 0.0 ;
        lv2:minimum -100.0 ;
        lv2:maximum 100.0 ;
        units:unit [
            rdfs:label   "Low/High" ;
            units:symbol "Lo <> Hi" ;
            units:render "%f";
        ];
        rdfs:comment "Feedback filtering - low-pass to left, high-pass to right"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "LFO Depth" ;
        lv2:shortName "Depth" ;
        lv2:symbol "lfo_depth" ;
        lv2:default 30.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc;
        rdfs:comment "Delay time modulation (sine wave)"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "LFO Rate" ;
        lv2:shortName "Rate" ;
        lv2:portProperty mod:tapTempo;
        lv2:symbol "lfo_rate" ;
        lv2:default 50.0 ;
        lv2:minimum 0.10 ;
        lv2:maximum 100.0 ;
        units:unit units:pc;
        rdfs:comment "Modulation rate"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "FX Mix" ;
        lv2:shortName "Mix" ;
        lv2:symbol "fx_mix" ;
        lv2:default 50.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc;
        rdfs:comment "Wet / dry mix"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Level" ;
        lv2:symbol "output" ;
        lv2:default 0.0 ;
        lv2:minimum -12.0 ;
        lv2:maximum 6.0 ;
        #lv2:scalePoint [
        #    rdfs:label "OFF" ;
        #    rdf:value -34.0
        #];
        units:unit units:db;
        rdfs:comment "Level trim"
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 7 ;
        lv2:symbol "left_in" ;
        lv2:name "Left In" ;
        lv2:designation pg:left ;
        pg:group mda:mainIn
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 8 ;
        lv2:symbol "right_in" ;
        lv2:name "Right In" ;
        lv2:designation pg:right ;
        pg:group mda:mainIn
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 9 ;
        lv2:symbol "left_out" ;
        lv2:name "Left Out" ;
        lv2:designation pg:left ;
        pg:group mda:mainOut
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 10 ;
        lv2:symbol "right_out" ;
        lv2:name "Right Out" ;
        lv2:designation pg:right ;
        pg:group mda:mainOut
    ] .
