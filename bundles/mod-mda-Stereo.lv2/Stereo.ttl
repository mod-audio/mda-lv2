@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix mda: <http://moddevices.com/plugins/mda/> .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

mda:Stereo
    a lv2:Plugin ,
        lv2:SpatialPlugin ;
    lv2:project mda: ;
    lv2:symbol "Stereo" ;
    doap:name "MDA Stereo" ;
    doap:shortdesc "Haas delay and comb filtering" ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput mda:mainIn ;
    pg:mainOutput mda:mainOut ;
    lv2:minorVersion 0;
    lv2:microVersion 2;
    mod:brand "MDA" ;
    mod:label "Stereo" ;

    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:name "Width" ;
        lv2:symbol "width" ;
        lv2:default 56.0 ;
        lv2:minimum -100.0 ;
        lv2:maximum 100.0 ;
        units:unit [
        a            units:Unit ;
        rdfs:label   "Haas/Comb" ;
        units:symbol "HAAS<>COMB" ;
        units:render "%f HAAS<>COMB"
        ];

    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:name "Delay" ;
        lv2:symbol "delay" ;
        lv2:default 22.43 ;
        lv2:minimum 0.45 ;
        lv2:maximum 47.62 ;
        units:unit units:ms;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "Balance" ;
        lv2:symbol "balance" ;
        lv2:default 0.0 ;
        lv2:minimum -100.0 ;
        lv2:maximum 100.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Mod" ;
        lv2:symbol "mod" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 47.62 ;
        lv2:scalePoint [
            rdfs:label "OFF" ;
            rdf:value 0.0
        ];
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Rate" ;
        lv2:symbol "rate" ;
        lv2:default 50.0 ;
        lv2:minimum 0.10 ;
        lv2:maximum 100.0 ;
        units:unit units:s;
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 5 ;
        lv2:symbol "left_in" ;
        lv2:name "Left In" ;
        lv2:designation pg:left ;
        pg:group mda:mainIn
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 6 ;
        lv2:symbol "right_in" ;
        lv2:name "Right In" ;
        lv2:designation pg:right ;
        pg:group mda:mainIn
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 7 ;
        lv2:symbol "left_out" ;
        lv2:name "Left Out" ;
        lv2:designation pg:left ;
        pg:group mda:mainOut
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 8 ;
        lv2:symbol "right_out" ;
        lv2:name "Right Out" ;
        lv2:designation pg:right ;
        pg:group mda:mainOut
    ] .