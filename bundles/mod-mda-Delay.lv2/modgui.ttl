@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix ui:     <http://lv2plug.in/ns/extensions/ui#> .

<http://moddevices.com/plugins/mda/Delay>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-delay.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-delay.css> ;
        modgui:screenshot <modgui/screenshot-mda-delay.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-delay.png> ;
        modgui:brand "MDA" ;
        modgui:label "Delay" ;
        modgui:model "boxy" ;
        modgui:panel "8-knobs" ;
        modgui:color "white" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "l_delay" ;
            lv2:name "L Delay" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "r_delay" ;
            lv2:name "R/L Delay" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "feedback" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "fb_tone" ;
            lv2:name "Fb Tone" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "fx_mix" ;
            lv2:name "FX Mix" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "output" ;
            lv2:name "Output" ;
        ] ;
    ] .