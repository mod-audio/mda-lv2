@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mda: <http://moddevices.com/plugins/mda/> .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

mda:Dynamics
    a lv2:Plugin ,
        lv2:DynamicsPlugin ;
    lv2:project mda: ;
    lv2:symbol "Dynamics" ;
    doap:name "MDA Dynamics" ;
    doap:shortdesc "Compressor / Limiter / Gate" ;
    rdfs:comment "Compressor / Limiter / Gate" ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput mda:mainIn ;
    pg:mainOutput mda:mainOut ;
    lv2:minorVersion 0;
    lv2:microVersion 2;
    mod:brand "MDA" ;
    mod:label "Dynamics" ;

    lv2:port[
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:name "Thresh" ;
        lv2:symbol "thresh" ;
        lv2:default -16.0 ;
        lv2:minimum -40.0 ;
        lv2:maximum 0.0 ;
        units:unit units:db
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:name "Ratio" ;
        lv2:symbol "ratio" ;
        lv2:default -10 ;
        lv2:minimum -17.0 ;
        lv2:maximum 0.5 ;
        units:unit [
            a            units:Unit ;
            rdfs:label   "ratio" ;
            units:symbol ":1" ;
            units:render "%f" ;
        ];
        rdfs:comment """Very wide range allows "overcompression" where output gets quieter as input gets louder"""
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "Output" ;
        lv2:symbol "output" ;
        lv2:default 4.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 40.0 ;
        units:unit units:db;
        rdfs:comment "Level trim"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Attack" ;
        lv2:symbol "attack" ;
        lv2:portProperty epp:logarithmic;
        lv2:default 283.14 ;
        lv2:minimum 2.0 ;
        lv2:maximum 1571.0 ;
        units:unit units:ms
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Release" ;
        lv2:symbol "release" ;
        lv2:portProperty epp:logarithmic;
        lv2:default 864.6 ;
        lv2:minimum 1.0 ;
        lv2:maximum 1571.0 ;
        units:unit units:ms
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Limiter" ;
        lv2:symbol "limiter" ;
        lv2:default 10.0 ;
        lv2:minimum -20.0 ;
        lv2:maximum 10.0 ;
        units:unit units:db;
        lv2:scalePoint [
            rdfs:label "OFF" ;
            rdf:value 10.0
        ];
        rdfs:comment "Limiter threshold - the limiter has zero attack time but uses the same release time as the compressor"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Gate Thr" ;
        lv2:symbol "gate_thr" ;
        lv2:default -60.0 ;
        lv2:minimum -60.0 ;
        lv2:maximum 0.0 ;
        units:unit units:db ;
        lv2:scalePoint [
            rdfs:label "OFF" ;
            rdf:value -60.0
        ]
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Gate Att" ;
        lv2:symbol "gate_att" ;
        lv2:portProperty epp:logarithmic;
        lv2:default 5.0 ;
        lv2:minimum 5.0 ;
        lv2:maximum 15782.0 ;
        units:unit units:ms
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Gate Rel" ;
        lv2:symbol "gate_rel" ;
        lv2:portProperty epp:logarithmic;
        lv2:default 173.93 ;
        lv2:minimum 9.0 ;
        lv2:maximum 17384.0 ;
        units:unit units:ms
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Mix" ;
        lv2:symbol "mix" ;
        lv2:default 100.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 10 ;
        lv2:symbol "left_in" ;
        lv2:name "Left In" ;
        lv2:designation pg:left ;
        pg:group mda:mainIn
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 11 ;
        lv2:symbol "right_in" ;
        lv2:name "Right In" ;
        lv2:designation pg:right ;
        pg:group mda:mainIn
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 12 ;
        lv2:symbol "left_out" ;
        lv2:name "Left Out" ;
        lv2:designation pg:left ;
        pg:group mda:mainOut
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 13 ;
        lv2:symbol "right_out" ;
        lv2:name "Right Out" ;
        lv2:designation pg:right ;
        pg:group mda:mainOut
    ] .
