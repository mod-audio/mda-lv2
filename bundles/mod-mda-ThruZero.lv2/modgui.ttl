@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mda/ThruZero>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-thruzero.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-thruzero.css> ;
        modgui:screenshot <modgui/screenshot-mda-thruzero.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-thruzero.png> ;
        modgui:brand "MDA" ;
        modgui:label "ThruZero" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "purple" ;
        modgui:knob "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "rate" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "depth" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "mix" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "feedback" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "depth_mod" ;
            lv2:name "Depth Mod" ;
        ] ;
    ] .