@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mda: <http://drobilla.net/plugins/mda/> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix modplugins: <http://portalmod.com/plugins/mda/> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<http://drobilla.net/plugins/mda/Piano/env>
    a param:EnvelopeControls ,
        pg:InputGroup ;
    lv2:name "Envelope" ;
    lv2:symbol "env" .

modplugins:Piano
    a lv2:Plugin ,
        lv2:InstrumentPlugin ;
    lv2:project mda: ;
    lv2:symbol "Piano" ;
    doap:name "MDA Piano" ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
    lv2:extensionData <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;
    pg:mainInput mda:mainIn ;
    pg:mainOutput mda:mainOut ;
    lv2:minorVersion 0;
    lv2:microVersion 1;
    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:name "Envelope Decay" ;
        lv2:symbol "env_decay" ;
        lv2:default 50.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc;
        lv2:designation param:decay ;
        pg:group <http://drobilla.net/plugins/mda/Piano/env>
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:name "Envelope Release" ;
        lv2:symbol "env_release" ;
        lv2:default 50.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc;
        lv2:designation param:release ;
        pg:group <http://drobilla.net/plugins/mda/Piano/env>
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "Hardness Offset" ;
        lv2:symbol "hardness_offset" ;
        lv2:default 0.0 ;
        lv2:minimum -50.0 ;
        lv2:maximum 50.0 ;
        rdfs:comment """Adjusts sample keyranges up or down to change the "size" and brightness of the piano. Use "V" slider to adjust velocity control"""
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Velocity to Hardness" ;
        lv2:symbol "vel_to_hardness" ;
        lv2:default 50.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "Muffling Filter" ;
        lv2:symbol "muffling_filter" ;
        lv2:default 80.3 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "Velocity to Muffling" ;
        lv2:symbol "vel_to_muffling" ;
        lv2:default 25.1 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Velocity Sensitivity" ;
        lv2:symbol "vel_sensitivity" ;
        lv2:default 37.6 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
        units:unit units:pc;
        rdfs:comment """Velocity curve: Mid point is normal "square law" response"""
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Stereo Width" ;
        lv2:symbol "stereo_width" ;
        lv2:default 100.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 200.0 ;
        units:unit units:pc
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Polyphony" ;
        lv2:symbol "polyphony" ;
        lv2:portProperty lv2:enumeration ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:scalePoint [
            rdfs:label "8 voices" ;
            rdf:value 0
        ] , [
            rdfs:label "32 voices" ;
            rdf:value 1
        ]
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Fine Tuning" ;
        lv2:symbol "fine_tuning" ;
        lv2:default 0.0 ;
        lv2:minimum -50.0 ;
        lv2:maximum 50.0 ;
        units:unit units:cent
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "Random Detuning" ;
        lv2:symbol "random_detuning" ;
        lv2:default 12.3 ;
        lv2:minimum 0.0 ;
        lv2:maximum 50.0 ;
        units:unit units:cent
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "Stretch Tuning" ;
        lv2:symbol "stretch_tuning" ;
        lv2:default 0.0 ;
        lv2:minimum -50.0 ;
        lv2:maximum 50.0 ;
        units:unit units:cent
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 12 ;
        lv2:symbol "left_out" ;
        lv2:name "Left Out" ;
        lv2:designation pg:left ;
        pg:group mda:mainOut
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 13 ;
        lv2:symbol "right_out" ;
        lv2:name "Right Out" ;
        lv2:designation pg:right ;
        pg:group mda:mainOut
    ] , [
        a lv2:InputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ;
        lv2:index 14 ;
        lv2:symbol "event_in" ;
        lv2:name "Event In"
    ] ;
     mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-mda-piano.html>;
        mod:templateData <modgui/data-mda-piano.json>;
        mod:screenshot <modgui/screenshot-mda-piano.png>;
        mod:thumbnail <modgui/thumb-mda-piano.png>;
    ] .
