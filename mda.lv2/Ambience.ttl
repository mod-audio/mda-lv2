@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mda: <http://drobilla.net/plugins/mda/> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix modplugins: <http://portalmod.com/plugins/mda/> .
@prefix mod: <http://portalmod.com/ns/modgui#> .
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.

modplugins:Ambience
    a lv2:Plugin ,
        lv2:ReverbPlugin ;
    lv2:project mda: ;
    lv2:symbol "Amb" ;
    doap:name "MDA Ambience" ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    pg:mainInput mda:mainIn ;
    pg:mainOutput mda:mainOut ;
    lv2:minorVersion 0;
    lv2:microVersion 3;

    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:name "Size" ;
        lv2:symbol "size" ;
        lv2:default 7 ;
        lv2:minimum 0 ;
        lv2:maximum 10 ;
        units:unit units:m ;

    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:name "HF Damp" ;
        lv2:symbol "hf_damp" ;
        lv2:default 70 ;
        lv2:minimum 0 ;
        lv2:maximum 100 ;
        units:unit units:pc ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "Mix" ;
        lv2:symbol "mix" ;
        lv2:default 90 ;
        lv2:minimum 0 ;
        lv2:maximum 100 ;
        units:unit units:pc ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Output" ;
        lv2:symbol "output" ;
        lv2:default 0 ;
        lv2:minimum -20 ;
        lv2:maximum 20 ;
        units:unit units:db ;
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 4 ;
        lv2:symbol "left_in" ;
        lv2:name "Left In" ;
        lv2:designation pg:left ;
        pg:group mda:mainIn
    ] , [
        a lv2:InputPort ,
            lv2:AudioPort ;
        lv2:index 5 ;
        lv2:symbol "right_in" ;
        lv2:name "Right In" ;
        lv2:designation pg:right ;
        pg:group mda:mainIn
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 6 ;
        lv2:symbol "left_out" ;
        lv2:name "Left Out" ;
        lv2:designation pg:left ;
        pg:group mda:mainOut
    ] , [
        a lv2:OutputPort ,
            lv2:AudioPort ;
        lv2:index 7 ;
        lv2:symbol "right_out" ;
        lv2:name "Right Out" ;
        lv2:designation pg:right ;
        pg:group mda:mainOut
    ] .
